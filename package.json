{
  "name": "node-js-api",
  "version": "1.0.0",
  "description": "API custom framework",
  "main": "main.js",
  "type": "commonjs",
  "scripts": {
    "start": "node ./dist/main.js",
    "dev": "nodemon",
    "dev:inspect": "nodemon -e ts,json --exec node --inspect=localhost:9222 -r ts-node/register ./src/main.ts",
    "lint": "eslint ./src/**",
    "lint:fix": "eslint ./src/** --fix",
    "generate": "prisma generate",
    "build": "tsc"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SergeiAmpilov/node-js-api.git"
  },
  "keywords": [
    "API",
    "node.js"
  ],
  "author": "Ampilov Sergei",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/SergeiAmpilov/node-js-api/issues"
  },
  "homepage": "https://github.com/SergeiAmpilov/node-js-api#readme",
  "dependencies": {
    "@prisma/client": "^4.11.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.20.2",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "inversify": "^6.0.1",
    "reflect-metadata": "^0.1.13",
    "tslog": "^4.7.2"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/express": "^4.17.17",
    "@typescript-eslint/eslint-plugin": "^5.54.0",
    "@typescript-eslint/parser": "^5.54.0",
    "eslint": "^8.35.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-prettier": "^4.2.1",
    "nodemon": "^2.0.20",
    "prettier": "^2.8.4",
    "prisma": "^4.11.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5"
  }
}
